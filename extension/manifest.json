{
  "name": "Chrome Verified Access API demo",
  "description": "Explore how Chrome Verified Access API can be used to verify your ChromeBook users and/or devices.",
  "version": "1.0.0",
  "manifest_version": 2,
  "permissions": [ "enterprise.platformKeys", "storage", "identity", "identity.email" ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "16": "images/cvaa-demo16.png",
      "32": "images/cvaa-demo32.png",
      "48": "images/cvaa-demo48.png",
      "128": "images/cvaa-demo128.png"
    }
  },
  "icons": {
    "16": "images/cvaa-demo16.png",
    "32": "images/cvaa-demo32.png",
    "48": "images/cvaa-demo48.png",
    "128": "images/cvaa-demo128.png"
  }
}
